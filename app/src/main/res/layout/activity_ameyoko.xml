<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ameyoko">

    <TextView
        android:id="@+id/textView81"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ameyoko"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="332dp" />

    <TextView
        android:id="@+id/textView82"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="There are many shops on the road of about 500m, and the cheap sale is the centerpiece. In particular, there are many shops that handle seafood so you can enjoy the traditional atmosphere. You can also have fun listening to the recommended sightseeing spots close to the store clerk."
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="402dp" />

    <Button
        android:id="@+id/button30"
        android:layout_width="76dp"
        android:layout_height="40dp"
        android:text="Button"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="553dp" />

    <TextView
        android:id="@+id/textView83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address: 4-14 Ueno 4-chome Taito-ku, Tokyo
Telephone: 0338325053
Business Hours: 10:00am-8:00pm
Website: http://www.ameyoko.net/
"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="641dp" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="314dp"
        android:layout_height="228dp"
        app:srcCompat="@drawable/ameyoko"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="70dp" />
</androidx.constraintlayout.widget.ConstraintLayout>